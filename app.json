{
  "expo": {
    "name": "Coffee Finder",
    "description": "This project is to help users find the their favorite coffee places and save them in for later. It can also be used to find the best coffee places aroud town.",
    "slug": "my-first-app",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/icon.png",
    "splash": {
      "image": "./src/assets/splash/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F8E9D6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      
      "supportsTablet": false
    }
  }
}
